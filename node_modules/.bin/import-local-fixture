#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/admin/Documents/learn-front-end/testLab/awesome-front-end/awesome-vite/smart-ui-vite/node_modules/import-local/fixtures/node_modules:/Users/admin/Documents/learn-front-end/testLab/awesome-front-end/awesome-vite/smart-ui-vite/node_modules/import-local/node_modules:/Users/admin/Documents/learn-front-end/testLab/awesome-front-end/awesome-vite/smart-ui-vite/node_modules:/Users/admin/Documents/learn-front-end/testLab/awesome-front-end/awesome-vite/node_modules:/Users/admin/Documents/learn-front-end/testLab/awesome-front-end/node_modules:/Users/admin/Documents/learn-front-end/testLab/node_modules:/Users/admin/Documents/learn-front-end/node_modules:/Users/admin/Documents/node_modules:/Users/admin/node_modules:/Users/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/admin/Documents/learn-front-end/testLab/awesome-front-end/awesome-vite/smart-ui-vite/node_modules/import-local/fixtures/node_modules:/Users/admin/Documents/learn-front-end/testLab/awesome-front-end/awesome-vite/smart-ui-vite/node_modules/import-local/node_modules:/Users/admin/Documents/learn-front-end/testLab/awesome-front-end/awesome-vite/smart-ui-vite/node_modules:/Users/admin/Documents/learn-front-end/testLab/awesome-front-end/awesome-vite/node_modules:/Users/admin/Documents/learn-front-end/testLab/awesome-front-end/node_modules:/Users/admin/Documents/learn-front-end/testLab/node_modules:/Users/admin/Documents/learn-front-end/node_modules:/Users/admin/Documents/node_modules:/Users/admin/node_modules:/Users/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../import-local/fixtures/cli.js" "$@"
else
  exec node  "$basedir/../import-local/fixtures/cli.js" "$@"
fi
